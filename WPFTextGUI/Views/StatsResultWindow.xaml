<Window x:Class="WPFTextGUI.Views.StatsResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFTextGUI.Views"
        mc:Ignorable="d"
        Title="Analysis results" Height="400" Width="400">
    <Grid>
        <local:StatsResultView Margin="0,0,0,60" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
        <Button Name="btnSend" Content="Send" 
                VerticalAlignment="Bottom" Width="100" 
                Height="30" FontSize="20" Margin="0,0,0,10"
                Click="btnSend_Click"/>
    </Grid>
</Window>
